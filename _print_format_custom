#include "main.h"

/**
 *_print_format_custom check the format ad print depends on it
 *@frmtsp the format specifier
 *@args arguments
 *return the number of printd character
*/
int _print_format_custom(char frmtsp, va_list args)
{
	int counter;
	counter = 0;
	if(frmtsp == '%')
		_print_char_custom(frmtsp);
	else if(frmtsp == 'c')
		_print_char_custom((va_args(args, int));
	else if(frmtsp == 's')
		_print_string_custom(vargs(args, char *));
	else if(frmtsp == 'i' || frmtsp == 'd')
		_print_int_decimal(vargs(args, int), 10);
}
